#include<stdio.h>
#include<string.h>
#include<iostream>
using namespace std;

void reverse_array(int arr[], int size) {
	for (int a = 0; a < (size - 1) / 2; a++) {
		int buf = arr[a];
		arr[a] = arr[size - 1 - a];
		arr[size - 1 - a] = buf;
	}
}

int main() {
	int a[] = { 4, 11, -1, 5 };
	reverse_array(a, 4);
	std::cout << a[0] << " " << a[1] << " " << a[2] << " " << a[3] << " " << "\n";


	return 0;

}
