#include<string.h>
#include<iostream>
using namespace std;

int string_length(char s[]) {
	int a;
	for (a = 0; s[a] != 0; a++);
	return a;
}


int main(){
	int a=string_length("hello");
	cout << a<<endl;
	a=string_length("");
	cout << a<<endl;


	char foo[20] = { 'H', 'e', 'l', 'l', 'o', '\0', 'a', 'b', '\0' };
	a=string_length(foo);
	cout << a<<endl;


	char fooo[20] = { 'H', 'e', 'l', 'l', 'o', 'c', 'a', 'b', '\0' };
	a=string_length(fooo);
	cout << a<<endl;
	return 0;
}
