

#include<stdio.h>
#include<string.h>
#include<iostream>
using namespace std;

bool angrams(char a[], char b[]){
	int alen, blen;
	for (alen = 0; a[alen] != 0; alen++);
	for (blen = 0; b[blen] != 0; blen++);
	if (alen != blen)return 0;

	for (int i = 0; i < alen; i++) {
		int z;
		for (z = 0; z < alen; z++)
			if (a[i] == b[z])break;
		if (z == alen)return 0;
	}
	return 1;
}

int main() {
	int a=angrams("hello", "elloh");
	cout << a<<endl;
	a=angrams("hello", "elleh");
	cout << a << endl;
	a=angrams("", "");
	cout << a << endl;


	return 0;

}
