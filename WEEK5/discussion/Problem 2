#include <iostream>
#include <cassert>
using namespace std;
string remove_extra_whitespace(string str_in) {
	string q="";
	for (int a = 0; a < str_in.size(); a++) {
		if (str_in[a] != ' ') {
			int pos = a;
			for (pos; pos<str_in.size()&& str_in[pos] != ' '; pos++);
			pos;
			for (int k = a; k < pos; k++) {
				q += str_in[a];
				++a;
			}
			q += ' ';
		}
	}
	int len = q.size()-1;
	q.erase(len);
	return q;
}

int main() {
	string a = remove_extra_whitespace("      a     g gg   ");
	printf("%s\n", a.c_str());




	return 0;
}
