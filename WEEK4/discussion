#include <iostream>
#include<stdio.h>
#include<string.h>
using namespace std;

bool isPrime(int number) {
	int ok = 1;
	for (int a = 2; a < number; a++) {
		if (number%a == 0) { ok = 0; break; }
	}
	return ok;
}

int main() {
	int a = 0;
	scanf("%d", &a);
	if (isPrime(a)) printf("%d is prime", a);
	else { printf("%d is not a prime", a); }
}





#include <iostream>
#include<stdio.h>
#include<string.h>
using namespace std;

bool isPrime(int number) {
	for (int a = 2; a < number; a++) {
		if (number%a == 0)
			return 0;
	}
	return 1;
}

bool isSumOfTwoPrine(int number) {
	for (int a = 2; a < (number +1)/ 2; a++) {
		if (isPrime(a)) {
			int b = number - a;
			if (isPrime(b))
				return 1;
		}
		return 0;
	}
}
int main() {
	int a;
	scanf("%d", &a);
	if(isSumOfTwoPrine(a)) printf("%d\n", a);
	else printf("not %d\n",a);
}

#include <iostream>
#include<stdio.h>
#include<string.h>
using namespace std;

int fib(int n) {
	if (n == 0)return 0;
	if (n == 1)return 1;
	if (n >= 2)return fib(n - 1) + fib(n - 2);
}

int main() {
	int a = fib(6);
	printf("%d", a);
}
#include <iostream>
#include<stdio.h>
#include<string.h>
using namespace std;

void addFunny(int & a, int  b)
{
	a = a * 2;
	a = a + b;
}

int main()
{
	int A = 5;
	cout << "A = " << A << endl;
	addFunny(A, A);

	cout <<"Result = " << A << endl;
	return 0;
}
